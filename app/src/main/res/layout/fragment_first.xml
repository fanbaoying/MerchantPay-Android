                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="16dp"
                            android:textColor="#838383"/>

                        <TextView
                            android:id="@+id/todaycontent1"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="0.00元"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="18dp"
                            android:textColor="#37383C"/>

                        <TextView
                            android:id="@+id/lab4"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="#eeeff2"/>

                    </LinearLayout>

                    <LinearLayout android:layout_width="1dp"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/lab5"
                            android:layout_width="match_parent"
                            android:layout_height="80dp"
                            android:background="#eeeff2"/>

                    </LinearLayout>


                    <LinearLayout android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/todaytitle2"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="交易笔数"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="16dp"
                            android:textColor="#838383"/>

                        <TextView
                            android:id="@+id/todaycontent2"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="0笔"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="18dp"
                            android:textColor="#37383C"/>

                        <TextView
                            android:id="@+id/lab6"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="#eeeff2"/>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal" >

                    <LinearLayout android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/todaytitle3"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="新增会员"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="16dp"
                            android:textColor="#838383"/>

                        <TextView
                            android:id="@+id/todaycontent3"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="0个"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="18dp"
                            android:textColor="#37383C"/>

                    </LinearLayout>

                    <LinearLayout android:layout_width="1dp"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/lab7"
                            android:layout_width="match_parent"
                            android:layout_height="80dp"
                            android:background="#eeeff2"/>

                    </LinearLayout>


                    <LinearLayout android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/todaytitle4"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="回头客"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="16dp"
                            android:textColor="#838383"/>

                        <TextView
                            android:id="@+id/todaycontent4"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="0个"
                            android:gravity="center_vertical"
                            android:layout_marginLeft="15dp"
                            android:textSize="18dp"
                            android:textColor="#37383C"/>

                    </LinearLayout>


                </LinearLayout>


                <LinearLayout android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/lab8"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:background="#eeeff2"/>

                </LinearLayout>



            </LinearLayout>


        </LinearLayout>
    </ScrollView>

</RelativeLayout>